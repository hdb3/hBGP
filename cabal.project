packages: hbgp.cabal
optimization:        2
profiling: False
jobs: 4
executable-stripping: True
package hbgp
  ghc-options: -threaded -with-rtsopts="-N"
--   ghc-options: -threaded -with-rtsopts="-N" -prof -fprof-auto  -fexternal-interpreter -rtsopts
--  ghc-options: -threaded -eventlog -with-rtsopts="-N -s -t -l"
